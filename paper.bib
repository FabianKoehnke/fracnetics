@article{KonstantinosLiagkouras.2018,
	abstract = {AbstractThis article examines the effect of different configuration issues of the Multiobjective Evolutionary Algorithms on the efficient frontier formulation for the constrained portfolio optimization problem. We present the most popular techniques for dealing with the complexities of the constrained portfolio optimization problem and experimentally analyse their strengths and weaknesses. In particular, we examine the efficient incorporation of complex real world constraints into the Multiobjective Evolutionary Algorithms and their corresponding effect on the efficient frontier formulation for the portfolio optimization problem. Moreover, we examine various constraint-handling approaches for the constrained portfolio optimization problem such as penalty functions and reparation operators and we draw conclusions about the efficacy of the examined approaches. We also examine the effect on the efficient frontier formulation by the application of different genetic operators and the relevant results are analysed. Finally, we address issues related with the various performance metrics that are applied for the evaluation of the derived solutions.},
	author = {{Konstantinos Liagkouras} and {Konstantinos Metaxiotis}},
	doi = {10.1057/jors.2016.38},
	journal = {Journal of the Operational Research Society},
	number = {3},
	pages = {416--438},
	title = {Examining the effect of different configuration issues of the multiobjective evolutionary algorithms on the efficient frontier formulation for the constrained portfolio optimization problem},
	volume = {69},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1057/jors.2016.38}}

